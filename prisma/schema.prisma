generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id                 Int      @id @default(autoincrement())
  email              String
  name               String?
  provider           String
  providerAccountId  String

  @@unique([provider, providerAccountId])
}
